  "author": "kirofint <kirofint@gmail.com>",
  "license": "MIT",
  "scripts": {
		"start": "concurrently -k -i -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,cyan.bold\" \"yarn watch-js\" \"yarn watch-ts\"",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/app.js"
  },
  "_moduleAliases": {
    "@commands": "dist/commands",
    "@helpers": "dist/helpers",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/mongoose": "^5.7.32",
    "@types/node": "^14.0.23",
    "dotenv": "^8.2.0",
    "module-alias": "^2.2.2",
    "mongoose": "5.10.18",
    "telegraf": "^3.38.0",
    "typescript": "^4.0.3"
  }
}
